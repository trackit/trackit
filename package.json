    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "~3.0.3",
    "babel-runtime": "6.26.0",
    "bootstrap": "~3.3.7",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "create-react-class": "15.6.2",
    "css-loader": "0.28.4",
    "d3": "~3.5.17",
    "dotenv": "4.0.0",
    "enzyme": "~3.1.0",
    "enzyme-adapter-react-16": "~1.0.2",
    "eslint": "4.4.1",
    "eslint-config-react-app": "~2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "express": "~4.16.2",
    "extract-text-webpack-plugin": "3.0.0",
    "fetch-mock": "~5.13.1",
    "file-loader": "0.11.2",
    "font-awesome": "~4.7.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "ify-loader": "~1.1.0",
    "jest": "20.0.4",
    "jest-enzyme": "~4.0.1",
    "jquery": "~3.2.1",
    "material-ui": "1.0.0-beta.27",
    "moment": "2.19.2",
    "normalize.css": "~7.0.0",
    "nvd3": "~1.8.6",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "prop-types": "~15.6.0",
    "react": "~16.0.0",
    "react-bootstrap": "~0.31.5",
    "react-bootstrap-daterangepicker": "~3.4.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dev-utils": "~4.1.0",
    "react-dom": "~16.0.0",
    "react-nvd3": "0.5.7",
    "react-redux": "~5.0.6",
    "react-router-dom": "~4.2.2",
    "react-spinkit": "^3.0.0",
    "react-table": "~6.6.0",
    "react-test-renderer": "~16.0.0",
    "react-validation": "~3.0.6",
    "redux": "~3.7.2",
    "redux-saga": "~0.15.6",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "uuid": "^3.2.1",
    "validator": "~9.0.0",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "lint": "eslint src",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.js",
      "!src/**/gcp/***.js",
      "!src/common/Theme.js",
      "!src/{config|registerServiceWorker}.js",
      "!src/{actions|constants|store}/***.js",
      "!src/sagas/{watcher|misc}.js",
      "!src/api/auth.js",
      "!src/api/{aws}/*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/requestAnimationFrame.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "jest": true
    }
  }
}
